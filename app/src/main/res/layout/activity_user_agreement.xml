<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.yiwo.friendscometogether.pages.UserAgreementActivity">

    <RelativeLayout
        android:id="@+id/activity_user_agreement_rl_top"
        style="@style/rl_top_bar">

        <TextView
            android:id="@+id/ctivity_user_agreement_tv_title"
            style="@style/tv_top_title"
            android:text="用户协议" />

        <RelativeLayout
            android:id="@+id/activity_user_agreement_rl_back"
            android:layout_width="100px"
            android:layout_height="match_parent">

            <ImageView style="@style/iv_return" />

        </RelativeLayout>

    </RelativeLayout>

    <WebView
        android:id="@+id/activity_user_agreement_webview"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
</LinearLayout>
